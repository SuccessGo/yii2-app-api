{
    "name": "success/yii2-app-api",
    "description": "Yii2 Api Application Template",
    "keywords": [
        "yii2",
        "framework",
        "api",
        "project template"
    ],
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Success Go",
            "email": "successgdc@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "7.3.* || 7.4.*",
        "yiisoft/yii2": "2.0.42"
    },
    "require-dev": {
    },
    "provide": {
        "bower-asset/inputmask": "*",
        "bower-asset/jquery": "*",
        "bower-asset/punycode": "*",
        "bower-asset/yii2-pjax": "*"
    },
    "autoload": {
        "psr-4": {
            "api\\": "api",
            "common\\": "common",
            "console\\": "console"
        }
    },
    "config": {
        "sort-packages": true
    }
}
